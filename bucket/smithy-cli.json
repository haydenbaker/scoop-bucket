{
    "version": "1.43.0",
    "description": "Smithy CLI - A CLI for building, validating, querying, and iterating on Smithy models",
    "homepage": "https://smithy.io",
    "license": "Apache-2.0",
    "url": "https://github.com/haydenbaker/smithy/releases/download/1.43.0/smithy-cli-windows-x64.zip",
    "hash": "$url.sha256",
    "extract_dir": "smithy-cli-windows-x64",
    "env_add_path": "bin",
    "checkver": {
      "url": "https://github.com/haydenbaker/smithy/releases/latest",
      "re": "v([\\d.]+).zip"
    },
    "autoupdate": {
      "url": "https://github.com/haydenbaker/smithy/releases/download/$version/smithy-cli-windows-x64.zip",
      "extract_dir": "smithy-cli-windows-x64",
      "hash": {
        "url": "$url.sha256"
      }
    }
  }
  